{
  "name": "web-proto",
  "version": "1.0.0",
  "description": "A scratchpad for prototyping HTML, JS, and CSS.",
  "main": "index.js",
  "scripts": {
    "clean": "node scripts/init.js",
    "init": "node scripts/init.js && chmod +x scripts/*.sh && scripts/transform-gitignore.sh && scripts/init-git.sh",
    "start": "browser-sync start --config ./bs-config.js",
    "update": "degit github:xbc5/proto-mc-type-face --force && pnpm update; scripts/transform-gitignore.sh"
  },
  "keywords": [
    "js",
    "web",
    "css",
    "html",
    "practice",
    "prototyping",
    "refresh"
  ],
  "author": "xbc5",
  "license": "MIT",
  "dependencies": {
    "browser-sync": "^2.27.7",
    "@fsouza/prettierd": "^0.18.1",
    "fs-extra": "^10.0.0"
  }
}
